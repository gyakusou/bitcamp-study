<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:p="http://www.springframework.org/schema/p"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
    
    <bean id="c1" class="com.eomcs.spring.ioc.ex07.Car">
        <property name="model" value="티코"/> <!-- String - 문자열 -->
        <property name="maker" value="비트자동차"/> <!-- String - 문자열 -->
        <property name="cc" value="890"/> <!-- int - 문자열(Integer.praseInt로 형변환) -->
        <property name="auto" value="true"/> <!-- boolean - 문자열(boolean.parseBoolean로 형변환) -->
        
        <property name="createdDate" value="2018-5-8"/>
        
    </bean>
</beans>






